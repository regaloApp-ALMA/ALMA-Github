{
  "expo": {
    "name": "ALMA",
    "slug": "alma-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "alma",
    "userInterfaceStyle": "automatic",
    "backgroundColor": "#ffffff",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.rork.arbol-de-vida-alma-k2bf03xp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "app.rork.arbol-de-vida-alma-k2bf03xp"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "title": "ALMA: Tu legado digital",
      "name": "ALMA",
      "shortName": "ALMA",
      "display": "standalone",
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      }
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://rork.app/"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}